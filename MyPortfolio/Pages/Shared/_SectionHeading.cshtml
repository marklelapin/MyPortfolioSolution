
@model MyPortfolio.Pages.Shared._SectionHeadingModel
@{
}
<!-- Heading -->
<div class="position-relative d-flex text-center mb-@Model.MarginBottom mt-@Model.MarginTop">
    <h2 class="text-24 text-white-50 opacity-1 text-uppercase fw-600 w-100 mb-0">@Model.SectionTitle</h2>
    <p class="text-9 text-white fw-600 position-absolute w-100 align-self-center lh-base mb-0">
        @Model.Title
    <span class="heading-separator-line border-bottom border-3 border-primary d-block mx-auto"></span> </p>
</div>
<!-- Heading end-->